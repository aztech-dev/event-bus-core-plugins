{
    "name": "aztech/event-bus-core-plugins",
    "description": "aztech/event-bus core plugins collection",
    "keywords": [
        "event",
        "events",
        "dispatch",
        "aztech"
    ],
    "type": "library",
    "autoload": {
        "psr-4": {
            "Aztech\\Events\\Bus\\Plugins\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "require": {
        "aztech/event-bus": "~1",
        "psr/log": "~1.0"
    },
    "require-dev": {
        "aztech/coding-standard": "~0.1",
        "mikey179/vfsStream": "~1",
        "phpunit/phpunit": "~4.2",
        "squizlabs/php_codesniffer": "~1.5",
        "vektah/bugfree-dangerzone": "0.2.0"
    },
    "suggest": {
        "ext-mysql": "To use the PDO transport plugin with MySQL",
        "ext-pgsql": "To use the PDO transport with PostGres"
    },
    "extra": {
        "branch-alias": {
            "dev-master": "1.1.x-dev"
        }
    }
}
